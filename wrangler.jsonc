{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "amanah-go",
  "compatibility_date": "2024-12-27",
  "pages_build_output_dir": "dist",
  "compatibility_flags": ["nodejs_compat"],
  "vars": {
    "ENVIRONMENT": "production",
    "GOOGLE_CLIENT_ID": "",
    "FACEBOOK_APP_ID": ""
  },
  "r2_buckets": [
    {
      "binding": "R2",
      "bucket_name": "amanah-go-storage"
    }
  ]
}
